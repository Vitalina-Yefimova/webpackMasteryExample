{
  "parserOptions": {
    "ecmaVersion": 2023, // Обновление до новейшей версии ECMAScript для пожжержания динамичных импортов
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off",
    "sort-keys": "warn"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended"
  ]
}